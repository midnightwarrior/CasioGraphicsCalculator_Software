Data Type:PG
Capacity:334
File Name:-.CMPRS2
Group Name:
Password:
Option1:NL
Option2:
Option3:
Option4:
%Data Record
'APPEND MAT E TO BITSTRING TO BE COMPRESSED
\Lbl 0:\For 1\->V \To 7
\Mat E[1,V]\->\Mat D[1,V+U]
\Next 
U\>=49\=>\Goto1
U+7\->U
\Return
\Lbl 1
'STORE BITSTRING
0\->U
0\->V
0\->Z
1\->E
\For 1\->V \To 49
Z+((2^(V-1))*\Mat D[1,50-V])\->Z
\Next 
'50-EXPONENT SIGN
\Mat D[1,50]=0\=>\Goto2
\Mat D[1,50]=1\=>\Goto3
\Lbl 2:\(-)1\->E
\Lbl 3
'51-56 EXP
Z*10^\(-)14\->Z
0\->F
\For 51\->V \To 56
F+((2^(V-51))*\Mat D[1,V]\->F
\Next 
IZ\10^x(EF)\->\Mat C[1,W]
W+1\->W
\Fill(0,\Mat D)
\Return
%End
%Header Record
Format:TXT
Communication SW:0
Data Type:PG
Capacity:517
File Name:-.DCMPRS
Group Name:
Password:
Option1:NL
Option2:
Option3:
Option4:
%Data Record
\Fill(0,\Mat D)
'DECOMPRESSOR
'LOAD FROM MEMORY ONTO SCREEN
0\->\Mat A[1,5]
\For 1\->V \To 255
\Fill(0,\Mat D)
\Mat C[1,V]\->B
B=0\=>\Stop
'GET MANTISSA SIGN
B<0\->\Mat D[1,57]
'GET EXPONENT
\Abs B\->Q
\Intg (\log Q)\->E
(Q/(10^(\Intg \log Q)))\10^x14\->Q
'CONVERT MANTISSA TO BIN
\For 48\->C \To 0 \Step \(-)1
\Int (Q/(2^C))\->\Mat D[1,49-C]
(Q-\Int (Q/(2^C))*2^C\->Q
\Next 
'GET EXPONENT
E>0\->\Mat D[1,50]
\Abs E\->E
'BIN EXPONENT
\For 5\->D \To 0 \Step \(-)1
\Int (E/(2^D))\->\Mat D[1,51+D]
\Frac (E/(2^D))*2^D\->E
\Next 
'GET 'BIN TO CHARS
\For 0\->M \To 49 \Step 7
0\->Z
\For 0\->C \To 6
Z+(\Mat D[1,1+C+M]*2^(6-C))\->Z
\Next 
Z\->\theta
\Prog "-.ASCSCR"
\Next 
256(\Mat D[1,57]=1)+V(\Mat D[1,57]\<>1)\->V
\Next 
\Locate 1,7,"DONE!"
%End
